    <children>
        <AnchorPane prefHeight="94.0" prefWidth="1024.0" style="-fx-background-color: rgb(179,179,179);">
            <children>
                <ImageView fitHeight="75.0" fitWidth="206.0" layoutX="14.0" layoutY="8.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@images/logo.png" />
                    </image>
                </ImageView>
                <Button fx:id="btnLogout" layoutX="841.0" layoutY="24.0" mnemonicParsing="false" onAction="#logout" prefHeight="40.0" prefWidth="143.0" style="-fx-background-color: rgb(179,179,179); -fx-border-color: rgb(0,0,200);" textAlignment="CENTER" textFill="#0000c8">
                    <opaqueInsets>
                        <Insets />
                    </opaqueInsets>
                </Button>
            </children>
        </AnchorPane>
        <ListView fx:id="lvAllProducts" layoutX="201.0" layoutY="144.0" onMouseClicked="#productSelected" prefHeight="397.0" prefWidth="634.0" />
      <Label fx:id="lbItems" layoutX="33.0" layoutY="129.0" text="Label" />
    </children>
</AnchorPane>
